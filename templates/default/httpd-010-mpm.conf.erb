# Generated by Chef for <%= node['hostname'] %>.
# Local modifications will be overwritten.

<IfModule mpm_prefork_module>
    StartServers        <%= node['apache2-wrapper']['prefork']['start_servers'] %>
    MinSpareServers     <%= node['apache2-wrapper']['prefork']['min_spare_servers'] %>
    MaxSpareServers     <%= node['apache2-wrapper']['prefork']['max_spare_servers'] %>
    MaxClients          <%= node['apache2-wrapper']['prefork']['max_clients'] %>
    MaxRequestsPerChild <%= node['apache2-wrapper']['prefork']['max_requests_per_child'] %>
</IfModule>

<IfModule mpm_worker_module>
    ServerLimit         <%= node['apache2-wrapper']['worker']['server_limit'] %>
    ThreadLimit         <%= node['apache2-wrapper']['worker']['thread_limit'] %>
    ThreadsPerChild     <%= node['apache2-wrapper']['worker']['threads_per_child'] %>
    MaxClients          <%= node['apache2-wrapper']['worker']['max_clients'] %>
    MinSpareThreads     <%= node['apache2-wrapper']['worker']['min_spare_threads'] %>
    MaxSpareThreads     <%= node['apache2-wrapper']['worker']['max_spare_threads'] %>
    MaxRequestsPerChild <%= node['apache2-wrapper']['worker']['max_requests_per_child'] %>
</IfModule>
